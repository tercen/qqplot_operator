{
  "name": "Q-Q Plot",
  "description": "Displays a Quantile-Quantile Plot to compare distribution quantiles against one another.",
  "tags": [
    "data visualisation",
    "plot",
    "statistical testing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/qqplot_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "distribution",
      "defaultValue": "normal",
      "values": [
        "normal",
        "uniform"
      ],
      "description": "Whichi distribution to compare to, if no x axis is provided."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "1.0.0",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Row dimension factors",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Column dimension factors",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Y-Axis",
                "type": "numeric",
                "description": "First distribution values",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1..n",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "X-Axis",
                "type": "numeric",
                "description": "Second distribution values (optional)",
                "ontologyMapping": "measurement",
                "crosstabMapping": "x",
                "cardinality": "0..1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": []
  }
}
