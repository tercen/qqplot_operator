{
  "name": "Q-Q Plot",
  "description": "Displays a Quantile-Quantile Plot to compare distribution quantiles against one another.",
  "tags": [
    "data visualisation",
    "plot",
    "statistical testing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/qqplot_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "distribution",
      "defaultValue": "normal",
      "values": [
        "normal",
        "uniform"
      ],
      "description": "Whichi distribution to compare to, if no x axis is provided."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor for grouping",
            "ontologyMapping": "row",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor for grouping",
            "ontologyMapping": "column",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Y Axis",
                "type": "numeric",
                "description": "Input values for the Y axis.",
                "ontologyMapping": "y-axis",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "X Axis",
                "type": "numeric",
                "description": "Variable to compare to the Y axis (optional).",
                "ontologyMapping": "x-axis",
                "crosstabMapping": "x",
                "cardinality": "0..1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}